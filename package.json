{
  "name": "tasks-app-nextjs",
  "version": "1.0.0",
  "repository": "git@github.com:macchiitaka/tasks-app-nextjs.git",
  "license": "MIT",
  "author": "macchiitaka <zxcvbnmtacashi@gmail.com>",
  "scripts": {
    "build": "pathpida --enableStatic && next build",
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:path": "pathpida --enableStatic --watch",
    "fmt": "run-s fmt:prettier fmt:package-json fmt:eslint",
    "fmt:prettier": "prettier --write --tab-width 2 \"**/*.{js,jsx,ts,tsx,json,yml,yaml,md}\"",
    "fmt:eslint": "eslint \"**/*.{js,jsx,ts,tsx}\" --cache --cache-strategy content --report-unused-disable-directives --fix",
    "fmt:package-json": "sort-package-json",
    "postinstall": "run-p postinstall:**",
    "lint": "run-p lint:**",
    "lint:eslint": "eslint \"**/*.{js,jsx,ts,tsx}\" --cache --cache-strategy content",
    "lint:type-check": "tsc -p . --noEmit",
    "postinstall:husky": "is-ci || husky install;",
    "postinstall:pathpida": "pathpida --enableStatic",
    "start": "next start"
  },
  "dependencies": {
    "@vanilla-extract/babel-plugin": "^1.1.3",
    "@vanilla-extract/css": "^1.6.8",
    "@vanilla-extract/css-utils": "^0.1.2",
    "@vanilla-extract/next-plugin": "^1.0.1",
    "@vanilla-extract/sprinkles": "^1.3.3",
    "axios": "0.24.0",
    "next": "12.0.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-is": "17.0.2",
    "react-query": "3.34.0"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^12.0.4",
    "@takamachi/eslint-config": "^2.3.0",
    "@takamachi/prettier-config": "^1.1.0",
    "@types/is-ci": "3.0.0",
    "@types/prettier": "^2.4.2",
    "@types/react": "17.0.37",
    "@types/react-dom": "17.0.11",
    "@types/react-is": "17.0.3",
    "@types/styled-components": "5.1.16",
    "eslint": "^8.4.0",
    "eslint-config-next": "12.0.4",
    "eslint-plugin-compat": "4.0.0",
    "eslint-plugin-jest": "25.3.0",
    "eslint-plugin-jest-dom": "3.9.2",
    "eslint-plugin-testing-library": "5.0.1",
    "husky": "7.0.4",
    "is-ci": "3.0.1",
    "lint-staged": "12.1.2",
    "npm-run-all": "4.1.5",
    "pathpida": "0.17.0",
    "pinst": "2.1.6",
    "prettier": "2.5.1",
    "sort-package-json": "1.53.1",
    "typescript": "4.5.2",
    "utility-types": "3.10.0"
  },
  "engines": {
    "node": ">=12.18.x",
    "yarn": ">=1"
  },
  "volta": {
    "node": "16.13.1",
    "yarn": "1.22.17"
  }
}
