  "license": "MIT",
  "author": "macchiitaka <zxcvbnmtacashi@gmail.com>",
  "scripts": {
    "build": "pathpida --enableStatic && next build",
    "dev": "run-p 'dev:*'",
    "dev:next": "next dev",
    "dev:path": "pathpida --enableStatic --watch",
    "fmt": "run-s fmt:prettier fmt:package-json fmt:eslint",
    "fmt:prettier": "prettier --write --tab-width 2 \"**/*.{js,jsx,ts,tsx,json,yml,yaml,md}\"",
    "fmt:eslint": "eslint \"**/*.{js,jsx,ts,tsx}\" --cache --cache-strategy content --report-unused-disable-directives --fix",
    "fmt:package-json": "sort-package-json",
    "lint": "run-p 'lint:*'",
    "lint:eslint": "eslint \"**/*.{js,jsx,ts,tsx}\" --cache --cache-strategy content",
    "lint:type-check": "tsc -p . --noEmit",
    "start": "next start"
  },
  "resolutions": {
    "browserslist": "4.20.3",
    "caniuse-lite": "1.0.30001334",
    "eslint-plugin-react-hooks": "4.5.0"
  },
  "dependencies": {
    "@next/bundle-analyzer": "12.1.6",
    "@vanilla-extract/babel-plugin": "1.1.6",
    "@vanilla-extract/css": "1.7.0",
    "@vanilla-extract/next-plugin": "2.0.2",
    "axios": "0.27.2",
    "constate": "3.3.2",
    "next": "12.1.6",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-is": "18.1.0",
    "react-query": "3.38.1"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "12.1.6",
    "@takamachi/eslint-config": "2.6.9",
    "@takamachi/prettier-config": "1.1.0",
    "@types/eslint": "8.4.1",
    "@types/prettier": "2.6.0",
    "@types/react": "18.0.8",
    "@types/react-dom": "18.0.3",
    "@types/react-is": "17.0.3",
    "browserslist": "4.20.3",
    "caniuse-lite": "1.0.30001334",
    "eslint": "8.14.0",
    "eslint-config-next": "12.1.6",
    "eslint-plugin-compat": "4.0.2",
    "npm-run-all": "4.1.5",
    "pathpida": "0.18.1",
    "prettier": "2.6.2",
    "sort-package-json": "1.57.0",
    "typescript": "4.6.4",
    "utility-types": "3.10.0"
  },
  "packageManager": "yarn@3.2.0",
  "engines": {
    "node": ">=12.18.x",
    "yarn": ">=1"
  },
